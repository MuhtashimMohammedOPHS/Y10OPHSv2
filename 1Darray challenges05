Food = []                     

ask = str(input("Enter a type of food >>>"))
ask2 = str(input("Enter a type of food>>>"))
ask3 = str(input("Enter a type of food>>>"))
ask4 = str(input("Enter a type of food>>>"))

Food.append(ask)
Food.append(ask2)
Food.append(ask3)
Food.append(ask4)

print(Food)

remove = str(input("Which food do you want to remove? >>>"))

Food.remove(remove)
  
print(Food)
